 <iati-activity default-currency="USD" dstore:dataset="usaid-mz" dstore:index="331" hierarchy="1" iati-activities:generated-datetime="2021-03-24T13:50:43.129Z" iati-activities:version="2.03" last-updated-datetime="2021-03-24T13:50:43.129Z" xml:lang="EN" xmlns:dstore="http://d-portal.org/xmlns/dstore" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities" xmlns:usg="https://explorer.usaid.gov">

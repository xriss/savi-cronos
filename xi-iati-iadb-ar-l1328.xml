  <title>
   <narrative xml:lang="EN">Global Credit Program for Reactivation of the Productive Sector</narrative>
   <narrative xml:lang="ES">Programa Global de Cr&#xE9;dito para la Reactivaci&#xF3;n del Sector Productivo</narrative>
   <narrative xml:lang="PT">N/A</narrative>
   <narrative xml:lang="FR">N/A</narrative>

 <iati-activity default-currency="USD" dstore:dataset="worldbank-ga" dstore:index="10" hierarchy="1" iati-activities:generated-datetime="2021-02-25T16:17:54" iati-activities:version="2.03" last-updated-datetime="2021-02-22T21:17:11" xml:lang="EN" xmlns:dstore="http://d-portal.org/xmlns/dstore" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities">

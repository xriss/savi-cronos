 <iati-activity default-currency="USD" dstore:dataset="worldbank-rs" dstore:index="21" hierarchy="1" iati-activities:generated-datetime="2020-12-27T23:13:38" iati-activities:version="2.03" last-updated-datetime="2020-12-23T14:31:07" xml:lang="EN" xmlns:dstore="http://d-portal.org/xmlns/dstore" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities">

 <iati-activity default-currency="DKK" dstore:dataset="drc-activities" dstore:index="26" hierarchy="1" humanitarian="1" iati-activities:generated-datetime="2020-10-21T16:24:58+00:00" iati-activities:version="2.03" last-updated-datetime="2020-10-12T08:16:25+00:00" xml:lang="EN" xmlns:dstore="http://d-portal.org/xmlns/dstore" xmlns:iati-activities="http://d-portal.org/xmlns/iati-activities">
